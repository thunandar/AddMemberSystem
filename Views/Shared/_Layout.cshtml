    <script src="~/Content/plugins/input-mask/jquery.inputmask.extensions.js" asp-append-version="true"></script>
    <script src="~/Content/bower_components/morris.js/morris.min.js" asp-append-version="true"></script>
    <!-- jQuery 3 -->
    <script src="~/Content/bower_components/jquery/dist/jquery.min.js" asp-append-version="true"></script>
    <!-- jQuery UI 1.11.4 -->
    <script src="~/Content/bower_components/jquery-ui/jquery-ui.min.js" asp-append-version="true"></script>
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script>
        $.widget.bridge('uibutton', $.ui.button);
    </script>
    <!-- Bootstrap 3.3.7 -->
    <script src="~/Content/bower_components/bootstrap/dist/js/bootstrap.min.js" asp-append-version="true"></script>
    <!-- Morris.js charts -->
    <script src="~/Content/bower_components/raphael/raphael.min.js" asp-append-version="true"></script>
    <script src="~/Content/bower_components/morris.js/morris.min.js" asp-append-version="true"></script>
    <!-- Sparkline -->
    <script src="~/Content/bower_components/jquery-sparkline/dist/jquery.sparkline.min.js" asp-append-version="true"></script>
    <!-- jvectormap -->
    <script src="~/Content/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js" asp-append-version="true"></script>
    <script src="~/Content/plugins/jvectormap/jquery-jvectormap-world-mill-en.js" asp-append-version="true"></script>
    <!-- jQuery Knob Chart -->
    <script src="~/Content/bower_components/jquery-knob/dist/jquery.knob.min.js" asp-append-version="true"></script>
    <!-- daterangepicker -->
    <script src="~/Content/bower_components/moment/min/moment.min.js" asp-append-version="true"></script>
    <script src="~/Content/bower_components/bootstrap-daterangepicker/daterangepicker.js" asp-append-version="true"></script>
    <!-- datepicker -->
    <script src="~/Content/bower_components/bootstrap-datepicker/dist/js/bootstrap-datepicker.min.js" asp-append-version="true"></script>
    <!-- Bootstrap WYSIHTML5 -->
    <!-- Slimscroll -->
    <script src="~/Content/bower_components/jquery-slimscroll/jquery.slimscroll.min.js" asp-append-version="true"></script>
    <!-- FastClick -->
    <script src="~/Content/bower_components/fastclick/lib/fastclick.js" asp-append-version="true"></script>
    <!-- AdminLTE App -->
    <script src="~/Content/dist/js/adminlte.min.js" asp-append-version="true"></script>
    <!-- AdminLTE dashboard demo (This is only for demo purposes) -->
    <!-- AdminLTE for demo purposes -->
    <script src="~/Content/bower_components/jquery-ui/jquery-ui.js" asp-append-version="true"></script>
    <script src="~/Content/scripts/Chart.min.js" asp-append-version="true"></script>
    <script src="~/Content/scripts/Chart.min.js" asp-append-version="true"></script>
    <script src="~/Content/plugins/toastr/toastr.min.js" asp-append-version="true"></script>

    <script src="~/Content/bower_components/moment/locale/my.js"></script>
    <script src="~/js/sweetalert2@11.js"></script>

    <!--my script Select box-->
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    <script>
        function EnToMy(EnStr) {
            return EnStr.replace('1', '၁').replace('2', '၂').replace('3', '၃').replace('4', '၄').replace('5', '၅').replace('6', '၆').replace('7', '၇').replace('8', '၈').replace('9', '၉').replace('0', '၀');
        }
        function MyToEn(MyStr) {
            return EnStr.replace('၁', '1').replace('၂', '2').replace('၃', '3').replace('၄', '4').replace('၅', '5').replace('၆', '6').replace('၇', '7').replace('၈', '8').replace('၉', '9').replace('၀', '0');
        }
    </script>
</body>
@RenderSection("Scripts", required: false)
</html>
