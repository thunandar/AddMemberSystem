<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>YRTC</title>

    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <link rel="stylesheet" href="~/Content/css/style_edit.css" asp-append-version="true">

    <!-- Select Box -->
    <link rel="stylesheet" href="~/Content/plugins/select2/select2.min.css" asp-append-version="true">

    <link href="~/Content/dist/css/fontawesome.min.css" rel="stylesheet" asp-append-version="true" />

    <!-- Bootstrap 3.3.7 -->
    <link rel="stylesheet" href="~/Content/bower_components/bootstrap/dist/css/bootstrap.min.css" asp-append-version="true">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/Content/bower_components/font-awesome/css/font-awesome.min.css" asp-append-version="true">
    <!-- Ionicons -->
    <link rel="stylesheet" href="~/Content/bower_components/Ionicons/css/ionicons.min.css" asp-append-version="true">
    <!-- Theme style -->
    <link rel="stylesheet" href="~/Content/dist/css/AdminLTE.min.css">

    <link rel="stylesheet" href="~/Content/dist/css/skins/_all-skins.css" asp-append-version="true">

    <link rel="stylesheet" href="~/Content/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css" asp-append-version="true">

    <link rel="stylesheet" href="~/Content/css/responsive.css" asp-append-version="true">


    <link rel="stylesheet" href="~/Content/css/profile_effect.css" asp-append-version="true">



    <link rel="icon" href="~/Content/Logo/YRTCRemovebg.png" type="image/png" sizes="16x16" />


</head>

<body class="hold-transition skin-yellow-light sidebar-mini">
    <div class="wrapper">      
        <header class="main-header" style="background-color: #6c85c3; height: 53px;">
            <div>
                <img src="~/Content/Logo/YRTCRemovebg.png" alt="YRTC Logo" style="width: 36px; height: 36px; margin-left: 4%; margin-top: -13px;"> <a style="font-size: 24px;
    font-weight: 600;
    color: white;
    display: inline-block;
    margin-top: 13px;">YRTC HR</a>
            </div>
            <nav class="navbar navbar-static-top" id="my-navbar" style="margin-top: -48px;">
                <div class="upNav">
                    <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
                        <span class="sr-only">Toggle navigation</span>
                    </a>
                    <div class="navbar-custom-menu" style="float:left">
                        <ul class="nav navbar-nav" style="float:left">
                            <li class="dropdown user user-menu" style="font-size:10px;">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                    <span class="hidden-xs" style="font-size:15px;">Test1 </span>
                                </a>
                                
                            </li>
                        </ul>
                    </div>
                    <div class="navbar-custom-menu">
                        <ul class="nav navbar-nav" style="float:right">
                            <li class="dropdown user user-menu" style="width:80px;">
                                <a href="#" class="dropdown-toggle display-picture" data-toggle="dropdown">
                                    <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRrTq97frFCSb9F3q2xKfP3-NGZZjzLBTqQhfL1V5v9Lg&amp;usqp=CAU&amp;ec=48600113" alt="Profile Image" style="width: 24px; height: 24px; border-radius: 50%; background-color: rgb(217, 217, 217); aspect-ratio: 1 / 1;"/>
                                   
                                    <div class="card hidden">
                                        <!--ADD TOGGLE HIDDEN CLASS ATTRIBUTE HERE-->
                                        <ul>
                                            <!--MENU-->
                                            <li> <a class="active" asp-area="" asp-controller="Home" asp-action="List">Staff</a></li>
                                            <li>
                                                <a class="active" asp-area="Setting" asp-controller="Department" asp-action="Index">Department</a>
                                            </li>
                                            <li>
                                                <a class="active" asp-area="Setting" asp-controller="Position" asp-action="Index">Position</a>
                                            </li>
                                            <li><a class="active" asp-area="" asp-controller="Account" asp-action="Logout">Logout</a></li>
                                        </ul>
                                    </div>
                    </a>
                                <ul class="dropdown-menu">
                                    @{
                                    }
                                </ul>
                            </li>
                        </ul>
                    </div>
                </div>
            </nav>
        </header>
        <aside class="main-sidebar">
            <section class="sidebar">
                <ul class="sidebar-menu" data-widget="tree" style="margin-top:60px;">
                    @{
                        <li class="treeview active menu-open">
                            <a href="#" class="active">
                                <i class="fa fa-list"></i>
                                <span style="font-size:16px;">MainMenu</span>
                                <span class="pull-right-container">
                                    <i class="fa fa-angle-left pull-right"></i>
                                </span>
                            </a>
                            <ul class="treeview-menu">
                                <li class="nav-item">
                                    <a class="active" asp-area="" asp-controller="Home" asp-action="Index" style="font-size:14px;"><i class="bi bi-house-door"></i> Home</a>
                                </li>
                                
                                <li class="nav-item">
                                    <a class="active" asp-area="" asp-controller="Home" asp-action="List" style="font-size:14px;"><i class="bi bi-people"></i> Staff</a>
                                </li>                               
                                <li class="nav-item">
                                    <a class="active" asp-area="" asp-controller="StaffLeave" asp-action="List" style="font-size:14px;" id="login-link"><i class="bi bi-calendar-date"></i> Leave</a>
                                </li>
                                <li class="nav-item">
                                    <a class="active" asp-area="" asp-controller="StaffPunishment" asp-action="List" style="font-size:14px;" id="login-link"><i class="bi bi-exclamation-square-fill"></i> Punishment</a>
                                </li>               
                                <li class="nav-item">
                                    <a class="active" asp-area="" asp-controller="Account" asp-action="Logout" style="font-size:14px;" id="login-link"><i class="bi bi-box-arrow-right"></i> Logout</a>
                                </li>
                               
                            </ul>
                        </li>
                        <li class="treeview active menu-open">
                            <a href="#" class="active">
                                <i class="fa fa-list"></i>
                                <span style="font-size: 16px;">Setting</span>

                                <span class="pull-right-container">
                                    <i class="fa fa-angle-left pull-right"></i>
                                </span>
                            </a>
                            <ul class="treeview-menu">
                                <li class="nav-item">
                                    <a class="active" asp-area="Setting" asp-controller="Department" asp-action="Index" style="font-size:14px;"><i class="bi bi-buildings"></i> Department</a>
                                </li>
                                <li class="nav-item">
                                    <a class="active" asp-area="Setting" asp-controller="Position" asp-action="Index" style="font-size:14px;"><i class="bi bi-person-badge"></i> Current Position</a>
                                </li>
                                <li class="nav-item">
                                    <a class="active" asp-area="Setting" asp-controller="InitialPosition" asp-action="Index" style="font-size:14px;"><i class="bi bi-person-badge"></i> Initial Position</a>
                                </li>
                                <li class="nav-item">
                                    <a class="active" asp-area="Setting" asp-controller="LeaveType" asp-action="Index" style="font-size:14px;" id="login-link"> <i class="bi bi-calendar-minus-fill"></i> Leave Type</a>
                                </li>
                                <li class="nav-item">
                                    <a class="active" asp-area="Setting" asp-controller="PunishmentType" asp-action="Index" style="font-size:14px;" id="login-link"> <i class="bi bi-calendar-minus-fill"></i> Punishment Type</a>
                                </li>
                            </ul> 
                        </li>
                    }
                </ul>
            </section>
        </aside>
        <div class="content-wrapper">
            <section class="content">
                @RenderBody()
            </section>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->
        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                <b>Version</b> 1.1.0
            </div>
            <strong>Copyright &copy; @DateTime.Now.Year <a href="http://acedatasystems.com/" target="_blank">YRTC</a>.</strong> All rights
            reserved.
        </footer>
        <!-- Control Sidebar -->
        <aside class="control-sidebar control-sidebar-dark">
            <!-- Create the tabs -->
            <ul class="nav nav-tabs nav-justified control-sidebar-tabs"></ul>
            <!-- Tab panes -->
            <div class="tab-content">
                <!-- Home tab content -->
                <div class="tab-pane" id="control-sidebar-home-tab">

                    <!-- /.control-sidebar-menu -->
                </div>
            </div>
        </aside>

        <div class="control-sidebar-bg"></div>
    </div>

    <!-- ./wrapper -->
    <script src="~/Content/plugins/jQuery/jquery-2.2.3.min.js" asp-append-version="true"></script>
    <script src="~/Content/plugins/select2/select2.full.min.js" asp-append-version="true"></script>
    <script src="~/Content/plugins/bootstrap-datepicker/bootstrap-datepicker.min.js" asp-append-version="true"></script>
    <script src="~/Content/plugins/datatables/jquery.dataTables.min.js" asp-append-version="true"></script>
    <script src="~/Content/plugins/datatables/dataTables.bootstrap.min.js" asp-append-version="true"></script>
    <script src="~/Content/plugins/datatables/dataTables.bootstrap.js" asp-append-version="true"></script>
    <script src="~/Content/plugins/input-mask/jquery.inputmask.js" asp-append-version="true"></script>
